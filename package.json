{
	"name": "turboplay",
	"version": "0.1.0",
	"private": true,
	"devDependencies": {
		"awesome-node-loader": "^1.1.1",
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.6",
		"babel-loader": "^7.1.5",
		"babel-plugin-transform-builtin-extend": "^1.1.2",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.5",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"css-loader": "^3.2.0",
		"electron-builder": "^21.2.0",
		"electron-packager": "^14.0.4",
		"eslint": "^4.18.0",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-plugin-class-property": "^1.1.0",
		"eslint-plugin-import": "^2.17.2",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-prettier": "^2.6.0",
		"eslint-plugin-react": "^7.13.0",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"node-sass": "^4.12.0",
		"prettier": "1.18.2",
		"react-hot-loader": "^4.12.11",
		"sass-loader": "^7.3.1",
		"style-loader": "^1.0.0",
		"uglifyjs-webpack-plugin": "^1.3.0",
		"url-loader": "^2.1.0",
		"webpack": "^4.39.2",
		"webpack-cli": "^3.3.7",
		"webpack-dev-server": "^3.8.0"
	},
	"scripts": {
		"web": "yarn web:pre && yarn web:post",
		"web:pre": "webpack-dev-server --config web/webpack.config.js",
		"web:post": "",
		"build-web": "yarn build-web:pre && yarn build-web:post",
		"build-web:pre": "webpack --config web/webpack.prod.config.js",
		"build-web:post": "yarn cp-assets",
		"build-electron": "yarn build-electron:pre && yarn build-electron:post",
		"build-electron:pre": "webpack --config web/webpack.electron.config.js",
		"build-electron:post": "yarn cp-assets && yarn cp-electron && yarn cp-brwc",
		"cp-assets": "rm -rf web/dist/assets && cp -R src/assets web/dist/",
		"cp-electron": "rm -rf electron/build && rm -rf tmp-build && mkdir tmp-build && cp -R electron/* tmp-build/ && cp -R web/dist/* tmp-build/ && mv tmp-build electron/build",
		"cp-brwc": "mkdir electron/build/brwc && cp -R src/brwc/BRWC.exe electron/build/brwc/BRWC.exe",
		"lint": "eslint '**/*.{js,jsx}'",
		"format": "prettier --write '**/*.{js,jsx}'",
		"pack": "build --dir",
		"electron": "node node_modules/electron/cli.js electron/",
		"package-mac": "electron-builder --dir",
		"package-win": "electron-packager electron/build turboplay --asar --overwrite --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=TurboPlay --version-string.FileDescription=CE --version-string.ProductName=\"TurboPlay\" --icon=\"electron/build/assets/images/TurboPlay.ico\""
	},
	"dependencies": {
		"@aedart/js-ioc": "^1.2.2",
		"bignumber.js": "^9.0.0",
		"cleave.js": "^1.5.3",
		"country-region-data": "^1.4.6",
		"debug": "^4.1.1",
		"electron": "^6.0.4",
		"filesize": "^4.1.2",
		"find-process": "^1.4.2",
		"js-cookie": "^2.2.1",
		"lodash": "^4.17.15",
		"mobx": "^3.5.1",
		"mobx-react": "^4.4.2",
		"mobx-react-router": "^4.0.7",
		"moment": "^2.24.0",
		"normalize.css": "^8.0.1",
		"numeral": "^2.0.6",
		"phoenix-socket": "^1.2.3",
		"prop-types": "^15.7.2",
		"query-string": "^6.8.2",
		"react": "16.4",
		"react-datepicker": "^1.5.0",
		"react-dom": "^16.8.6",
		"react-html5video": "^2.5.1",
		"react-modal": "^3.10.1",
		"react-router": "^4.3.1",
		"react-router-dom": "^4.3.1",
		"react-select": "^2.4.4",
		"react-slick": "^0.25.2",
		"react-timeout": "^1.2.0",
		"react-toast-notifications": "^1.5.0",
		"react-tooltip": "^3.10.0",
		"react-transition-group": "^2.9.0",
		"react-waypoint": "^9.0.2",
		"valid-card-number": "^1.0.2",
		"whatwg-fetch": "^3.0.0",
		"wolfy87-eventemitter": "^5.2.6"
	},
	"engines": {
		"node": ">=8.0.0"
	},
	"browser": {
		"fs": false,
		"child_process": false
	},
	"build": {
		"appId": "com.app.turboplay",
		"directories": {
			"output": "dist",
			"app": "electron/build",
			"buildResources": "electron/build"
		},
		"win": {
			"target": "nsis",
			"asar": false,
			"icon": "electron/build/assets/images/TurboPlay.ico"
		}
	},
	"license": "SEE LICENSE IN LICENSE.txt"
}
